{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "strictNullChecks": false,
    "noImplicitReturns": false
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ]
}

